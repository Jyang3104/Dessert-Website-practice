<table id="clerkTable">
    <thead>
        <th>Title</th>
        <th>Category</th>
        <th>Best Seller</th>
        <th>Price</th>
        <th>Quantity</th>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.cate}}</td>
            <td>{{this.best}}</td>
            <td>{{this.price}}</td>
            <td>{{this.qty}}</td>
            <td>
                <a href="/clerk/edit/{{this.id}}">EDIT</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

<a href="/clerk/add">Add product</a>